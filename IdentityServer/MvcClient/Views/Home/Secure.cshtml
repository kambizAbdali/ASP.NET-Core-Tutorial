@{
    ViewData["Title"] = "Secure Page";
}

<h2>User Claims</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

<div class="mt-3">
    <a href="/" class="btn btn-secondary">Back to Home</a>
</div>